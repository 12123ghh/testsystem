<% provide(:title,'all papers') %>
<%= render 'admin/layouts/header' %>
<h1>用户列表</h1>
  <ul class="users">
  	<% @papers.each do |paper| %>
  	  <li>
  	  	<%= paper.title %>
  	  	|<%= link_to "detail" , admin_paper_path(paper) %>
  	  	|<%= link_to "deleted",admin_papers_path(paper),method: :deleted,data:{confirm:"You sure?"} %>
  	  </li>	
  	<% end %>
  </ul>
