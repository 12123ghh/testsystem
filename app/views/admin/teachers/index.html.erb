<% provide(:title,'all teachers') %>
<%= render 'admin/layouts/header' %>
<h1>用户列表</h1>
  <ul class="users">
  	<% @users.each do |user| %>
  	  <li>
  	  	<%= user.name %>
  	  	|<%= link_to "detail" , admin_teacher_path(user) %>
  	  	|<%= link_to "deleted",admin_teachers_path(user),method: :deleted ,data:{confirm:"You sure?"} %>
  	  </li>	
  	<% end %>
  </ul>
