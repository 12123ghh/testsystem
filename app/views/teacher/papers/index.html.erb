<%= render 'teacher/layouts/header' %>

<table>
	<thead>
		<th>title</th>
		<th>subject</th>
		<th>status</th>
		<th>edit</th>
		<th>destroy</th>
	</thead>
	<% @papers.each do |paper| %>
		<tr>
			<td><%= paper.title %></td>
			<td><%= paper.subject %></td>
			<td><%= paper.review %></td>
			<td><%= link_to "edit", edit_teacher_paper_path(paper) %></td>
			<td><%= link_to "delete", teacher_paper_path(paper), method: :delete, data: { confirm: "are you sure?"} %></td>
		</tr>
	<% end %>
</table>
