<%= render "teacher/layouts/header" %>
<h1>Student examinations</h1>
<table class="table">
	<thead>
		<th>paper</th>
		<th>subject</th>
		<th>student</th>
		<th>achievement</th>
	</thead>
	<% @exams.each do |exam| %>
		<% if exam.user != nil %>
		<tr>
			<td><%= exam.paper.title %></td>
			<td><%= exam.paper.subject.name %></td>
			<td><%= exam.user.name %></td>
			<td><%= exam.calculate_score %></td>
		</tr>
		<% end %>
	<% end %>
</table>
<%= will_paginate @exams %>
<p><%= link_to "back", teacher_papers_path %></p>